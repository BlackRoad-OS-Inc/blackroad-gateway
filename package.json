{
  "name": "@blackroad/gateway",
  "version": "1.0.0",
  "description": "BlackRoad OS tokenless AI gateway",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers/ollama": "./dist/providers/ollama.js",
    "./providers/anthropic": "./dist/providers/anthropic.js",
    "./providers/openai": "./dist/providers/openai.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node --test tests/*.test.js 2>/dev/null || echo 'no compiled tests yet'",
    "lint": "eslint src/ --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackRoad-OS-Inc/blackroad-gateway.git"
  }
}
